<synthetic xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd"
           xmlns="http://www.xebialabs.com/deployit/synthetic" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <type type="atom.nugetTrigger" extends="xlrelease.ReleaseTrigger" >
    <property name="feedUrl" category="input" required="true" />
    <property name="proxy_string" category="input" required="false" />
    <property name="user" category="input" required="true" />
    <property name="password" category="input" required="true" kind="string" password="true" />
    <property name="packageName" category="input" required="true" />
    <property name="preRelease" category="input" kind="boolean" default="false" required="false" />
    <property name="nugetVersionForUse" category="variables" required="false" />
  </type>
</synthetic>
